@model prjMaribelMejia.Models.Modulos;

<h2>Edición de Módulo</h2>
    

<form asp-action="EditarRegistroModulo">
    <div>
        <label>Seleccione Propietario:</label>
    <input type="text" class="form-control idprop" name="IdPropietario" value="@Model.IdPropietario"/>
    <label>Modulo:</label>
    <input type="text" class="form-control nombremodulo" name="Modulo" value="@Model.Modulo"/>
    <label>Descripción del Módulo:</label>
    <input type="text" class="form-control descmodulo" name="DescripcionModulo" value="@Model.DescripcionModulo"/>
   <input type="hidden" class="form-control " name="IdModulo" value="@Model.IdModulo"/>
   <br />
    <div class=form-group>
        <input type="submit" style="width: 85px;" title="Actualiza registro" value="Actualizar" class="btn btn-success"/>
        <a class="btn btn-info" style="width: 85px;" title="Retornar" asp-area="" asp-controller="Productos" asp-action="Productos">Cancelar</a>
    </div>
    </div>
</form>